@model IEnumerable<FileUploadOperation.Models.Fileimplementation>

<h1>List of files</h1>


    <table class="table" border="1">
        <thead>
            <tr>
                <th>Id</th>
                <th>Filetype</th>
                <th>FileName</th>
                @* <th>size</th> *@
                <th>Download</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                <td>@user.Id</td>
                <td>@user.Filepath</td>
                <td>@user.FileName</td>
                
                @* <td>@user.Download</td> *@

                <td>@Html.ActionLink("Download", "Downloadfile", new { filename=user.FileName,filetype=user.Filepath})</td>
                   
                </tr>
            }
        </tbody>
    </table>
