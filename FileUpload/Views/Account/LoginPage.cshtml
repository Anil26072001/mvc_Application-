@model FileUploadOperation.Models.UserLogin

@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Welcome to Login Page</title>


    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">

    <script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery-validation@1.19.3/dist/jquery.validate.min.js"></script>
    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
    <link rel="stylesheet" href="~/css/Login.css">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>

    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

</head>
<body>
    <div>@ViewBag.Message</div>
    <div class="container mt-3">
        <div class="row justify-content-center">

            <div class="col-md-4">
                <div class="Logincontainer">
                    <form id="LoginPage" method="post" class="card p-3w">

                        <h1 class="text-center mb-4">Welcome to Login Page</h1>
                        <div class="mb-3">
                            <label for="email" class="form-label"><b>Email</b></label>
                            <input type="email" name="Email" placeholder="Enter Email" class="form-control" id="id-email">
    
                            <div id="error"></div>
                            <span id="emailicon" class="material-icons material-icons-outlined Iconcolor" style="display:none">error_outline</span>

                            @* <span id="emailicon"></span> *@

                            
            
                        </div>
                        <br />
                        <div class="mb-3">
                            <label for="password" class="form-label"><b>Password</b></label>
                            <input type="text" name="Password" placeholder="Enter Password" class="form-control" id="password-id" />
                            <div id="error1"></div>
                            @* <span id="passwordicon"></span> *@
                            <span id="passwordicon" class="material-icons material-icons-outlined Iconcolor" style="display:none">error_outline</span>

                        </div>
                        <div class="form-group mt-4">
                            <input type="submit" value="Login" class="btn btn-primary " id="id-submit" />
                        </div>
                    </form>

              @*    <div class="mt-3">
                        <a href="@Url.Action("forgetpassword","Account")"email=anil@gmail.com"> Forgot password </a>
                    </div> *@



                    <div class="mt-3">
                        <a href="@Url.Action("forgetpassword","Account")?email=anil@gmail.com">Forgot password</a>
                    </div>

                    @* 
                    <div class="mt-3">
                        <a href="@Url.Action("forgetpassword","Account")"> Forgot password </a>
                    </div> *@

                    <div class="container-fluid bg-primary text-white">
                    <div class="mt-3 text-center">
                        <a href="@Url.Action("Registrationpage","Account")" class="btn btn-primary"> @* class="text-decoration-none"> *@Register</a>
                    </div>
                    </div>
                    <div class="mt-3 text-center">
                        <p style="color:Red">@ViewBag.email</p>
                    </div>
                </div>

            </div>
        </div>
    </div>

    <script src="~/js/Loginpage.js"></script>
    <link rel="stylesheet" href="~/css/Login.css">

</body>
</html>

















































































